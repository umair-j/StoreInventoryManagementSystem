<Window x:Class="assignment2EAD.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:assignment2EAD"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Width="400" Height="100" Content="Admin" FontSize="50" Name="adminBtn" Click="adminBtn_Click"></Button>
        <Button Grid.Row="1" Width="400" Height="100" Content="Customer" FontSize="50" Name="customerBtn" Click="customerBtn_Click"></Button>
    </Grid>
</Window>
